{
  "name": "tractor-plugin-features",
  "version": "0.2.0",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/phenomnomnominal/tractor-plugin-features.git"
  },
  "bugs": {
    "url": "https://github.com/phenomnomnominal/tractor-plugin-features/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/server/index.js",
  "watch": {
    "build": "src/**/*.js"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run lint && npm run cover && npm run compile && npm run bundle",
    "bundle": "webpack --config webpack.config.js",
    "compile": "babel src/server/ --out-dir dist/server/ --ignore *.spec.js",
    "cover": "istanbul cover --config .istanbulrc.json node_modules/mocha/bin/_mocha -- --opts ./.mocharc 'src/**/*.spec.js'",
    "lint": "eslint src/**/*.js; true",
    "test": "mocha --opts ./.mocharc 'src/**/*.spec.js'",
    "test:debug": "mocha --inspect --debug-brk --opts ./.mocharc 'src/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "css-loader": "^0.28.5",
    "dirty-chai": "^2.0.1",
    "eslint": "^4.6.1",
    "html-loader": "^0.5.1",
    "ineeda": "^0.11.0",
    "istanbul": "1.0.0-alpha.2",
    "lodash.flatten": "^4.4.0",
    "lodash.memoize": "^4.1.2",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2",
    "ng-annotate-loader": "^0.6.1",
    "npm-watch": "^0.2.0",
    "sinon": "^3.2.1",
    "sinon-chai": "^2.12.0",
    "webpack": "^3.5.6"
  },
  "dependencies": {
    "dedent": "^0.7.0",
    "esprima": "^4.0.0",
    "estemplate": "^0.5.1",
    "gherkin": "2.12.2",
    "lodash.escaperegexp": "^4.1.2",
    "stripcolorcodes": "^0.1.0",
    "tractor-error-handler": "^0.2.0",
    "tractor-file-structure": "^0.4.0-beta.0",
    "tractor-logger": "^0.1.0",
    "tractor-plugin-step-definitions": "^0.2.0"
  }
}
